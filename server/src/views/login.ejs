<%- include('partials/header'); -%>

<div class="container">
  <h1>Welcome</h1>
  <div class="error"></div>
  <a href="/register/admin"><button class="blue">Register</button></a>
  <form id="login-form">
    <label for="email"></label>
    <input type="email" id="email" name="email" placeholder="Email" />
    <label for="password"></label>
    <input
      type="password"
      id="password"
      name="password"
      placeholder="Password"
    />
    <button class="green" type="submit">Log In</button>
  </form>
</div>